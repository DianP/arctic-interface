    "backgroundMenu": {
      "dark": "#eef1ff",
      "light": "#eef1ff"
    },
    "border": {
      "dark": "lavenderPurple",
      "light": "#9f92cc"
    },
    "borderActive": {
      "dark": "softPink",
      "light": "#cc9ecc"
