{
  "$schema": "https://usearctic.sh/theme.json",
  "defs": {
    "lavenderPurple": "#c8b6ff",
    "softPink": "#ffc6ff",
    "pastelBlue": "#a0c4ff",
    "mintCream": "#caffbf",
    "peachPuff": "#ffd6a5",
    "lilac": "#d4a5d8",
    "babyBlue": "#bde0fe",
    "text": "#2b2d42",
    "textMuted": "#8d99ae"
  },
  "theme": {
