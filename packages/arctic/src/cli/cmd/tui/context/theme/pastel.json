      "dark": "lavenderPurple",
      "light": "#9f92cc"
    },
    "secondary": {
      "dark": "softPink",
      "light": "#cc9ecc"
    },
    "accent": {
      "dark": "pastelBlue",
      "light": "#809dcc"
    },
    "error": {
      "dark": "#ff6b9d",
      "light": "#cc5579"
    },
    "warning": {
      "dark": "peachPuff",
      "light": "#ccaa84"
    },
    "success": {
      "dark": "mintCream",
      "light": "#a1cc99"
    },
    "info": {
      "dark": "babyBlue",
      "light": "#97b3ca"
    },
    "text": {
      "dark": "#2b2d42",
      "light": "#2b2d42"
    },
    "textMuted": {
      "dark": "textMuted",
      "light": "#8d99ae"
    },
    "background": {
