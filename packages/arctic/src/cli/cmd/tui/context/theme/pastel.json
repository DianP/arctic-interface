      "light": "#2b2d42"
    },
    "syntaxComment": {
      "dark": "textMuted",
      "light": "#8d99ae"
    },
    "syntaxKeyword": {
      "dark": "lavenderPurple",
      "light": "#9f92cc"
    },
    "syntaxFunction": {
      "dark": "pastelBlue",
      "light": "#809dcc"
    },
    "syntaxVariable": {
      "dark": "softPink",
      "light": "#cc9ecc"
    },
    "syntaxString": {
      "dark": "mintCream",
      "light": "#a1cc99"
    },
    "syntaxNumber": {
      "dark": "peachPuff",
      "light": "#ccaa84"
    },
    "syntaxType": {
      "dark": "babyBlue",
      "light": "#97b3ca"
    },
    "syntaxOperator": {
      "dark": "textMuted",
      "light": "#8d99ae"
    },
    "syntaxPunctuation": {
      "dark": "text",
      "light": "#2b2d42"
    }
  }
}
